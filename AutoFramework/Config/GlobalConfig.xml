<?xml version="1.0" encoding="utf-8" ?>
<AutoFramework>
  <RunSettings>
    <AUT>https://www.freecrm.com/index.html</AUT>
    <!-- Possible flag Chrome,Firefox, IE-->
    <Browser>Firefox</Browser>
    <!-- Possible flag Smoke, Regression, Unit  -->
    <TestType>SmokeTest</TestType>
    <!-- Possible Flag Y, N -->
    <IsLog>Y</IsLog>
    <LogPath>D:\\EAAutoLogs\\</LogPath>
    <DriverPath>D:\\drivers\\</DriverPath>
    <BuildName>DemoBuild</BuildName>
    <!-- Possible Flag Y, N -->
    <IsReport>N</IsReport>
    <UserName>EAutoTeam</UserName>
    <!-- Possible flag Selenium, Specflow  -->
    <Technology>Selenium</Technology>
    <TimeOut>10</TimeOut>
  </RunSettings>
</AutoFramework>
